{
    "name": "@hooker-monster/core",
    "version": "0.0.4",
    "scripts": {
        "build": "tsc --build --clean && tsc --build",
        "demo": "tsx ./demo.ts",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "eslint \"src/**/*.ts\" --fix"
    },
    "dependencies": {
        "@krakerxyz/utility": "^1.0.0",
        "cross-fetch": "^4.0.0",
        "jsonpath-plus": "^10.2.0",
        "paho-mqtt": "^1.1.0"
    },
    "devDependencies": {
        "@krakerxyz/eslint-config": "^2",
        "eslint": "^9.14.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3"
    },
    "description": "Core library for webhooks, providing client and data transfer objects.",
    "keywords": [
        "webhook",
        "testing",
        "realtime",
        "client",
        "hooker"
    ],
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/KrakerXyz/hooker-core.git"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./dto/*": {
            "types": "./dist/dto/*.d.ts",
            "import": "./dist/dto/*.js"
        },
        "./api": {
            "types": "./dist/client/ApiClient.d.ts",
            "import": "./dist/client/ApiClient.js"
        },
        "./mqtt": {
            "types": "./dist/client/MqttClient.d.ts",
            "import": "./dist/client/MqttClient.js"
        }
    },
    "publishConfig": {
        "access": "public"
    }
}